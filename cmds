docker-compose exec web /bin/bash
docker-compose exec mysql mysql -u root -proot
docker-compose exec mysql mysql -h 127.0.0.1 -P 3306 -u root -proot
docker-compose exec mysql mysql -u root -proot dispositivo < back-end/database/db.sql

vagrant init ubuntu/trusty64
vagrant up

sudo apt-get update && sudo apt-get upgrade
sudo apt-get install build-essential linux-headers-`uname -r` virtualbox
sudo apt-get -t stretch-backports install vagrant